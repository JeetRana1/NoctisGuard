{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/callback/?$", "dest": "/api/index.js" },
    { "src": "^/invite-callback/?$", "dest": "/api/index.js" },
    { "src": "^/api/(.*)$", "dest": "/api/index.js" },
    { "src": "^/.*$", "dest": "/index.html" }
  ]
}