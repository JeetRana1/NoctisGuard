{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/auth",
      "destination": "/server.js"
    },
    {
      "source": "/callback",
      "destination": "/server.js"
    },
    {
      "source": "/logout",
      "destination": "/server.js"
    },
    {
      "source": "/bot-event",
      "destination": "/server.js"
    },
    {
      "source": "/invite-now",
      "destination": "/server.js"
    },
    {
      "source": "/invite-callback",
      "destination": "/server.js"
    },
    {
      "source": "/internal/(.*)",
      "destination": "/server.js"
    }
  ]
}